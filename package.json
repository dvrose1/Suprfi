    "prisma": "^6.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plaid-link": "^4.1.1",
    "resend": "^6.5.2",
    "twilio": "^5.10.4",
    "zod": "^4.1.12"
